{
	"global": {
		"info": {
			"name": "Chisel",
			"author": "TooManyMods",
			"version": "Horizon Beta 1.11.4",
			"description": "More decorative blocks for minecraft!",
			"icon": "Chisel/mod_icon.png"
		},
		"api": "CoreEngine"
	},
	"make": {
		"debugAbi": "armeabi-v7a",
		"abis": ["armeabi-v7a", "x86"],
		"linkNative": ["minecraftpe", "innercore", "nativejs"],
		"excludeFromRelease": [],
		"pushTo": "storage/emulated/0/games/horizon/packs/Inner_Core_Test/innercore/mods/Chisel",
		"ndkPath": null
	},
	"resources": [
		{
			"path": "Chisel/src/assets/ic_mod",
			"type": "resource_directory"
		},
		{
			"path": "Chisel/src/assets/ic_mod/ui",
			"type": "gui"
		},
		{
			"path": "Chisel/src/assets/minecraft/resource/*",
			"type": "minecraft_resource_pack"
		},
		{
			"path": "Chisel/src/assets/minecraft/behavior/*",
			"type": "minecraft_behavior_pack"
		}
	],
	"sources": [
		{
			"source": "Chisel/src/build/mod",
			"target": "main.js",
			"type": "main",
			"language": "typescript"
		},
		{
			"source": "Chisel/src/build/lib/*",
			"type": "library",
			"language": "javascript"
		},
		{
			"source": "Chisel/src/build/preloader",
			"target": "preloader.js",
			"type": "preloader",
			"language": "typescript"
		},
		{
			"source": "Chisel/src/launcher.js",
			"type": "launcher",
			"language": "javascript"
		}
	],
	"compile": [],
	"additional": [
		{
			"source": "Chisel/src/assets/root/*",
			"targetDir": "."
		},
		{
			"source": "Chisel/src/assets/chisel/*",
			"targetDir": "resources/chisel"
		},
		{
			"source": "Chisel/src/assets/chisel_guide/*",
			"targetDir": "resources/chisel_guide"
		}
	]
}
